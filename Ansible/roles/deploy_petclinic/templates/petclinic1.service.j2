[Unit]
Description=Spring Petclinic App
After=syslog.target
After=network.target[Service]
User={{ petclinic_user }}
Type=simple
[Service]
ExecStart=/usr/bin/java -jar '{{ petclinic_path }}' '{{ petclinic_arguments }}'
Restart=always
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=petclinic
[Install]
WantedBy=multi-user.target
